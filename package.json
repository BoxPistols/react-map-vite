{
  "name": "react-drone-vite",
  "version": "0.0.1",
  "repository": "git@github.com:BoxPistols/react-drone-vite.git",
  "author": "BoxPistols",
  "license": "UNLICENSED",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "format": "yarn biome format --write .",
    "lint": "yarn biome lint --write ./src",
    "fix": "yarn run lint && yarn run format",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.18",
    "@mui/material": "^5.15.18",
    "cli-table3": "^0.6.4",
    "init": "^0.1.2",
    "mapbox-gl": "^3.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "string-width": "^4.2.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.0",
    "@chromatic-com/storybook": "^1.4.0",
    "@storybook/addon-essentials": "^8.1.3",
    "@storybook/addon-interactions": "^8.1.3",
    "@storybook/addon-links": "^8.1.3",
    "@storybook/addon-onboarding": "^8.1.3",
    "@storybook/blocks": "^8.1.3",
    "@storybook/react": "^8.1.3",
    "@storybook/react-vite": "^8.1.3",
    "@storybook/test": "^8.1.3",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "globals": "^15.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.4",
    "postcss": "^8.4.38",
    "react-map-gl": "^7.1.7",
    "storybook": "^8.1.3",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": ["yarn run fix"]
  },
  "husky": {
    "hooks": {
      "post-checkout": "chmod +x .husky/pre-commit && yarn run prepare",
      "post-merge": "chmod +x .husky/pre-commit && yarn run prepare"
    }
  },
  "volta": {
    "node": "20.13.1",
    "yarn": "1.22.22"
  }
}
