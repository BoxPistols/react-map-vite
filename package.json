{
  "name": "@boxpistols/react-map-vite",
  "version": "0.0.10",
  "type": "module",
  "main": "./dist/react-map-vite.umd.js",
  "module": "./dist/react-map-vite.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "author": "BoxPistols",
  "description": "A react map component using maplibre-gl",
  "keywords": [
    "react",
    "map",
    "maplibre-gl"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/react-map-vite.es.js",
      "require": "./dist/react-map-vite.umd.js"
    },
    "./styles.css": "./dist/style.css"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "sideEffects": false,
  "scripts": {
    "build:lib": "pnpm run build:types && vite build --mode lib",
    "build:types": "tsc -p tsconfig.build.json",
    "prepublishOnly": "pnpm run build:lib",
    "build": "vite build",
    "dev": "vite",
    "preview": "vite preview",
    "format": "pnpm biome format --write .",
    "lint": "pnpm biome lint --write ./src",
    "fix": "pnpm run lint && pnpm run format",
    "sb": "storybook dev -p 6006",
    "build:dev": "pnpm run build",
    "build:sb": "storybook build",
    "build:all": "pnpm run build && pnpm run build:sb",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.6",
    "@mui/material": "^6.4.6",
    "@mui/system": "^6.4.6",
    "@storybook/preview-api": "^8.6.3",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/typography": "^0.5.16",
    "@types/maplibre-gl": "^1.14.0",
    "@types/react-router-dom": "^5.3.3",
    "cli-table3": "^0.6.5",
    "init": "^0.1.2",
    "map-or-similar": "^1.5.0",
    "mapbox-gl": "^3.10.0",
    "maplibre-gl": "^4.7.1",
    "memoizerific": "^1.11.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.0",
    "rollup": "^4.34.9",
    "string-width": "^4.2.3",
    "vercel": "^39.4.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@chromatic-com/storybook": "^3.2.5",
    "@emotion/babel-plugin": "^11.13.5",
    "@fullhuman/postcss-purgecss": "^6.0.0",
    "@storybook/addon-a11y": "^8.6.3",
    "@storybook/addon-essentials": "^8.6.3",
    "@storybook/addon-interactions": "^8.6.3",
    "@storybook/addon-links": "^8.6.3",
    "@storybook/addon-mdx-gfm": "^8.6.3",
    "@storybook/blocks": "^8.6.3",
    "@storybook/react": "^8.6.3",
    "@storybook/react-vite": "^8.6.3",
    "@storybook/test": "^8.6.3",
    "@tailwindcss/forms": "^0.5.10",
    "@types/node": "^20.17.23",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "esbuild": "^0.24.2",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "postcss": "^8.5.3",
    "react-map-gl": "^7.1.9",
    "storybook": "^8.6.3",
    "tailwindcss": "^3.4.17",
    "terser": "^5.39.0",
    "typescript": "^5.8.2",
    "vite": "^6.2.0",
    "vite-plugin-dts": "^4.5.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "pnpm run fix"
    ],
    "*.{css,scss}": [
      "pnpm run format"
    ]
  },
  "volta": {
    "node": "20.13.1"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "@biomejs/biome",
      "esbuild"
    ],
    "onlyBuiltDependencies": [
      "@biomejs/biome"
    ]
  }
}
