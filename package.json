{
  "name": "@boxpistols/react-map-vite",
  "version": "0.0.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/react-map-vite.umd.js",
  "module": "./dist/react-map-vite.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/react-map-vite.es.js",
      "require": "./dist/react-map-vite.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./style": "./dist/style.css"
  },
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --emitDeclarationOnly",
    "preview": "vite preview",
    "format": "pnpm biome format --write .",
    "lint": "pnpm biome lint --write ./src",
    "fix": "pnpm run lint && pnpm run format",
    "sb": "storybook dev -p 6006",
    "build:dev": "pnpm run build",
    "build:sb": "storybook build --quiet -o storybook-static",
    "build:all": "NODE_ENV=production pnpm run build && STORYBOOK_DISABLE_TELEMETRY=1 pnpm run build:sb --quiet",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/cache": "^11.13.5",
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.5",
    "@mui/icons-material": "^6.1.8",
    "@mui/material": "^6.1.8",
    "@mui/system": "^6.1.8",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/typography": "^0.5.15",
    "@types/maplibre-gl": "^1.14.0",
    "@types/react-router-dom": "^5.3.3",
    "cli-table3": "^0.6.5",
    "init": "^0.1.2",
    "map-or-similar": "^1.5.0",
    "mapbox-gl": "^3.8.0",
    "maplibre-gl": "^4.7.1",
    "memoizerific": "^1.11.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0",
    "rollup": "^4.27.3",
    "string-width": "^4.2.3",
    "vercel": "^39.1.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@chromatic-com/storybook": "^3.2.2",
    "@fullhuman/postcss-purgecss": "^6.0.0",
    "@storybook/addon-a11y": "^8.4.5",
    "@storybook/addon-essentials": "^8.4.5",
    "@storybook/addon-interactions": "^8.4.5",
    "@storybook/addon-links": "^8.4.5",
    "@storybook/addon-mdx-gfm": "^8.4.5",
    "@storybook/blocks": "^8.4.5",
    "@storybook/react": "^8.4.5",
    "@storybook/react-vite": "^8.4.5",
    "@storybook/test": "^8.4.5",
    "@tailwindcss/forms": "^0.5.9",
    "@types/node": "^20.11.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "esbuild": "^0.24.0",
    "globals": "^15.12.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.49",
    "react-map-gl": "^7.1.7",
    "storybook": "^8.4.5",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.6.3",
    "vite": "^5.4.11"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "pnpm run fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pnpm run fix",
      "post-checkout": "chmod +x .husky/pre-commit && pnpm run prepare",
      "post-merge": "chmod +x .husky/pre-commit && pnpm run prepare"
    }
  },
  "volta": {
    "node": "20.13.1"
  }
}
