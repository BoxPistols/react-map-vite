{
  "name": "react-drone-vite",
  "version": "0.0.1",
  "repository": "git@github.com:BoxPistols/react-drone-vite.git",
  "author": "BoxPistols",
  "license": "UNLICENSED",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "format": "bun biome format --write .",
    "lint": "bun biome lint --write ./src",
    "fix": "bun run lint && bun run format",
    "preview": "vite preview",
    "sb": "storybook dev -p 6006",
    "sb-build": "storybook build",
    "build-all": "bun run build && bun run sb-build",
    "prepare": "husky"
  },
  "dependencies": {
    "@emotion/cache": "^11.13.1",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.1",
    "@mui/material": "^6.1.1",
    "@mui/system": "^6.1.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/typography": "^0.5.13",
    "@types/maplibre-gl": "^1.14.0",
    "@types/react-router-dom": "^5.3.3",
    "cli-table3": "^0.6.4",
    "init": "^0.1.2",
    "map-or-similar": "^1.5.0",
    "mapbox-gl": "^3.3.0",
    "maplibre-gl": "^4.5.0",
    "memoizerific": "^1.11.3",
    "react": "^18.3.1",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.23.1",
    "string-width": "^4.2.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@chromatic-com/storybook": "^2.0.2",
    "@fullhuman/postcss-purgecss": "^6.0.0",
    "@storybook/addon-a11y": "^8.3.3",
    "@storybook/addon-essentials": "^8.3.3",
    "@storybook/addon-interactions": "^8.3.3",
    "@storybook/addon-links": "^8.3.3",
    "@storybook/addon-mdx-gfm": "^8.3.3",
    "@storybook/addon-onboarding": "^8.3.3",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/blocks": "^8.3.3",
    "@storybook/react": "^8.3.3",
    "@storybook/react-vite": "^8.3.3",
    "@storybook/test": "^8.3.3",
    "@tailwindcss/forms": "^0.5.7",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "globals": "^15.1.0",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.4",
    "postcss": "^8.4.38",
    "react-map-gl": "^7.1.7",
    "storybook": "^8.3.3",
    "tailwindcss": "^3.4.7",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": ["bun run fix"]
  },
  "husky": {
    "hooks": {
      "pre-commit": "bun run lint && bun run format",
      "post-checkout": "chmod +x .husky/pre-commit && bun run prepare",
      "post-merge": "chmod +x .husky/pre-commit && bun run prepare"
    }
  },
  "volta": {
    "node": "20.13.1"
  }
}
